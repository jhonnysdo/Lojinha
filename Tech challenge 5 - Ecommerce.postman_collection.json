	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"pm.sendRequest({",
					"    url: 'http://localhost:8080/api/v1/auth/signin',",
					"    method: 'POST',",
					"    header: {",
					"        'Content-Type': 'application/json'",
					"    },",
					"    body: {",
					"        mode: 'raw',",
					"        raw: JSON.stringify({",
					"            \"username\": \"otavio\",",
					"            \"password\": \"otavio\"",
					"        })",
					"    }",
					"}, (err, res) => {",
					"    if (err) {",
					"        console.log(err);",
					"    } else {",
					"        // Verifica se a resposta tem um corpo e se o status é de sucesso (200)",
					"        if (res && res.json && res.status === 200) {",
					"            try {",
					"                var responseJson = res.json();",
					"                pm.collectionVariables.set(\"TOKEN\", responseJson.token);",
					"            } catch (e) {",
					"                // Trata o caso em que o JSON não pode ser parseado ou o token não está presente",
					"                console.log(\"Erro ao parsear a resposta ou token não encontrado:\", e);",
					"            }",
					"        } else {",
					"            console.log(\"Resposta sem corpo ou status de erro:\", res.status);",
					"        }",
					"    }",
					"});",
					""
				]
			}
		},
